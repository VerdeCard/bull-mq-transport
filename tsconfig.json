{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "target": "es2019",
    "sourceMap": true,
    "outDir": "./dist",
    "incremental": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@verdecard/bull-mq-transport": [
        "libs/bull-mq-transport/src"
      ],
      "@verdecard/bull-mq-transport/*": [
        "libs/bull-mq-transport/src/*"
      ]
    }
  }
}